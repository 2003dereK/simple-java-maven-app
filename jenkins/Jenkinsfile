pipeline {
    agent any
    }
    stages {
        stage('Build') {
            steps {
                echo 'Building'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing'
            }
            post {
                always {
                    echo 'post always'
                }
            }
        }
        stage('Deliver') {
            steps {
                echo 'Delivering'
            }
        }
        stage('Maintain') {
            steps {
                echo 'Maintaining'
            }
        }
    }
}
